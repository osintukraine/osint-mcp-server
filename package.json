{
  "name": "osint-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for OSINT Intelligence Platform API",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "osint-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "inspector": "npx @anthropic-ai/mcp-inspector node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "osint",
    "telegram",
    "intelligence",
    "claude"
  ],
  "author": "OSINT Ukraine",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
